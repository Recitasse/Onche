<?xml version="1.0" ?>
<profil onche-version="3.8.4h" table="pined">
    <parameters>
        <settings engine="InnoDB" encoding="utf8mb4" collation="utf8mb4_general_ci"/>
        <comment>Répertorie les onchois badgés</comment>
    </parameters>
    <rows>
        <row name="id" type="INT" null="False" auto_increment="True"></row>
        <row name="userid" type="INT" null="False"></row>
        <row name="badgeid" type="INT" null="False"></row>
    </rows>
