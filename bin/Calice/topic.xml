<?xml version="1.0" ?>
<profil onche-version="3.8.4h" table="topic">
    <parameters>
        <settings engine="InnoDB" encoding="utf8mb4" collation="utf8mb4_general_ci"/>
        <comment>Tables des onchois</comment>
    </parameters>
    <rows>
        <row name="id" type="INT" null="False" auto_increment="True"></row>
        <row name="oid" type="INT" null="False"></row>
        <row name="operateur" type="INT" null="False"></row>
        <row name="nom" type="VARCHAR(3000)" null="False"></row>
        <row name="date" type="TIMESTAMP(2)" null="False" default="CURRENT_TIMESTAMP(2)"></row>
        <row name="message" type="INT" null="False"></row>
        <row name="lien" type="VARCHAR(1000)" null="False"></row>
        <row name="forum" type="TINYINT(15)" null="False"></row>
    </rows>
    <index>
        <unic-index>
            <name mode="ASC" visible="True">oid</name>
        </unic-index>
        <primary-key name="id"/>
        <foreign-index>
            <constraint name="createur">
                <foreign-key mode="ASC" visible="True">operateur</foreign-key>
                <reference table="onchois">id</reference>
                <on-delete>NO ACTION</on-delete>
                <on-update>NO ACTION</on-update>
            </constraint>
        </foreign-index>
    </index>
</profil>
