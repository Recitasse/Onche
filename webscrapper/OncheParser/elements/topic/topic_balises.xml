<?xml version="1.0" ?>
<balises onche-version="3.8.4h">
    <topic root="//div[@class='message']">
        <pseudo>
            <xpath>//a[@class='message-username']/text()</xpath>
            <clean></clean>
        </pseudo>
        <message is_message="1">
            <xpath>//div[@class='message-content']</xpath>
            <clean></clean>
            <contain></contain>
        </message>
        <signature is_message="1">
            <xpath>//div[@class='signature']</xpath>
            <clean></clean>
        </signature>
        <date type="date">
            <xpath>//div[@class='message-date']/@title</xpath>
            <clean re="^Publié le \d{2}/\d{2}/\d{4} à \d{2}:\d{2}:\d{2}$" time="1"></clean>
        </date>
    </topic>
    <table name="Onchois">
        <division name="pseudonyme">
        </division>
    </table>
</balises>