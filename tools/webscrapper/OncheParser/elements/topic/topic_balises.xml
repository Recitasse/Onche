<?xml version="1.0" ?>
<balises onche-version="3.8.4h">
    <topic root="//div[@class='messages']">
        <message>
            <pseudo>
                <css-selector>//div[@class='message']/@data-username</css-selector>
                <clean></clean>
            </pseudo>
            <message-id>
                <css-selector>//div[@class='message']/@data-id</css-selector>
                <clean></clean>
            </message-id>
            <message-content>
                <css-selector>//div[@class='message']/text()</css-selector>
                <clean></clean>
            </message-content>
            <date type="date">>
                <css-selector>//div[@class='message']/div[@class='message-bottom']/div[@class='message-date']/@title</css-selector>
                <clean></clean>
            </date>
        </message>
        <response>
            <css-selector>//div[@class='message']/div[@class='message answer']/@data-username</css-selector>
        </response>
    </topic>
    <table name="Onchois">
        <division name="pseudonyme">
        </division>
    </table>
</balises>